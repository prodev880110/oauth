    "express": "4.17.1",
    "fullstack-validator": "^1.0.0",
    "ip": "1.1.5",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "moment": "^2.26.0",
    "mongoose": "5.9.18",
    "morgan": "1.10.0",
    "multer": "1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
